@model WasteManager.Models.Customer

@{
    ViewData["Title"] = "Details";
}
@section Scripts{

    <script src="@Url.Page("https://maps.googleapis.com/maps/api/js?key="+APIkeys.Mapskey)"
            defer></script>
    <script>
